    srpna
  \else\ifnum\@month=9%
    září
  \else\ifnum\@month=10%
    října
  \else\ifnum\@month=11%
    listopadu
  \else\ifnum\@month=12%
    prosince
  \else
    <<neznámý měsíc (\the\@month)>>
  \fi\fi\fi\fi\fi\fi
  \fi\fi\fi\fi\fi\fi
