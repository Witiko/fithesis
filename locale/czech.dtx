\gdef\thesis@czech@formattedDate{{%
  \thesis@day.
  \newcount\@month\expandafter\@month\thesis@month\relax
  \ifnum\@month=1%
    ledna
  \else\ifnum\@month=2%
    února
  \else\ifnum\@month=3%
    března
  \else\ifnum\@month=4%
    dubna
